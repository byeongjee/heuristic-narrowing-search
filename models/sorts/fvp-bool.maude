
set include BOOL off . 

mod FVP-BOOL is
    sorts Bool' InitialBool' .
    subsort InitialBool' < Bool' .
    op false : -> InitialBool' [ctor] .
    op true : -> Bool' [ctor] .

    op _and_ : Bool' Bool' -> Bool' [assoc comm] .
    vars A B C : Bool' .

    eq true and A = A [variant] .
    eq false and A = false [variant] .
    eq A and A = A [variant] .

endm
