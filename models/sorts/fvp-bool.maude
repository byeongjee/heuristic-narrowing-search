
set include BOOL off . 

fmod FVP-BOOL is
    sort Boolean .
    ops false true : -> Boolean [ctor] .

    op _and_ : Boolean Boolean -> Boolean [assoc comm] .
    vars A B C : Boolean .

    eq true and A = A [variant] .
    eq false and A = false [variant] .
    eq A and A = A [variant] .

endfm
