fth TOSET< is
    including BOOL .

    sort Elt .
    op _<_ : Elt Elt -> Bool .
    op _>_ : Elt Elt -> Bool .
    vars E1 E2 E3 : Elt .
    eq E1 < E1 = false .
    ceq E1 < E3 = true if E1 < E2 and E2 < E3 [nonexec] .
    ceq E1 < E2 or E2 < E1 = true if E1 =/= E2 .
    eq E1 > E2 = E2 < E1 .
 endfth
